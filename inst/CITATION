citHeader("To cite hydrorecipes in publications use:")
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{hydrorecipes}: Additional Steps for the Analysis of Water Level Data",
         author = c(person("Jonathan", "Kennel"),
                    person("Beth", "Parker")),
         year = year,
         note = note,
         doi = "10.5281/zenodo.6773849",
         url = "https://github.com/jkennel/hydrorecipes")
